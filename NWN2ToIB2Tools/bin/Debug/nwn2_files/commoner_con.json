          "pcNode": false,
          "linkTo": 0,
          "ShowOnlyOnce": true,
          "NodeIsActive": true,
          "NodePortraitBitmap": "",
          "NodeNpcName": "",
          "conversationText": "<i>This person looks at you with a mixture of fear and awe.</i>",
          "IsExpanded": true,
          "subnodePointer": [],
          "syncStructs": [],
          "subNodes": [],
          "actions": [],
          "conditions": [],
