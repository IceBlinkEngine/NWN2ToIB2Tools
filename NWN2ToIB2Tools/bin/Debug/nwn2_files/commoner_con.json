          "linkTo": 0,
          "ShowOnlyOnce": false,
          "NodeIsActive": true,
          "NodePortraitBitmap": "",
          "NodeNpcName": "",
          "conversationText": "If Hamfast is the town's leader, Danmar is his council.",
          "IsExpanded": true,
          "subnodePointer": [],
          "syncStructs": [],
